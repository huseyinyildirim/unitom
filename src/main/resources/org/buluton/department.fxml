<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.buluton.DepartmentController">
    <children>
        <Label text="Öğrenci" />
        <Button fx:id="studentButton" text="Öğrenci" onAction="#switchToStudent" />
    </children>
    <children>
        <Label text="Personel" />
        <Button fx:id="staffButton" text="Öğrenci" onAction="#switchToStaff" />
    </children>
    <children>
        <Label text="Departman" />
        <Button fx:id="departmentButton" text="Departman" onAction="#switchToDepartment" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
